&output
 h5freq = 10
/

&domain
 origin = -1.5707963267948966, -1.5707963267948966
 extent = 3.141592653589793, 3.141592653589793
 grid = 21, 21
 bc = 'free slip', 'free slip'
/

&parcel
 n_per_cell = 4
 random = .true.
 seed = 42
 elliptic = .false.
/

&flow
type = TaylorGreen2D
A = 0.5
a = 2.0
B = -1.0
b = 1.0
s_x = 1.5707963267948966
s_y = 0.0
/

&interpolator
velocity = TrilinearVolWeighted
fields = TrilinearVolWeighted
/

&time
 limit = 2.0
 max_step = 0.025
 adaptive = .true.
/
